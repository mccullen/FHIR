<Window x:Class="SeniorProject.QuestionnaireWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Questionnaire" Height="400" Width="400">
	<ScrollViewer VerticalScrollBarVisibility="Auto">
		<StackPanel Name="questionsStackPanel">
			<Button Name="submitButton" DockPanel.Dock="Bottom" HorizontalAlignment="Center" Click="submitButton_Click">Submit</Button>
		</StackPanel>
	</ScrollViewer>
</Window>
